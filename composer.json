{
  "name"        : "shelob9/foundri-mvp",
  "description" : "foundri mvp.",
  "authors"     : [
    {
      "name"    : "Foundri",
      "email"   : "Josh@JoshPress.net",
      "homepage": "http://Foundri.co"
    }
  ],
  "type"        : "project",
  "minimum-stability": "dev",
  "repositories": [
    {
      "type": "composer",
      "url" : "http://wpackagist.org"
    },
    {
      "type": "git",
      "url": "https://github.com/pods-framework/pods"
    },
    {
      "type": "git",
      "url" : "https://bitbucket.org/calderadev/cf-geo-autocomplete"
    },
    {
      "type": "git",
      "url" : "https://bitbucket.org/calderadev/caldera-easy-pods"
    },
    {
      "type": "git",
      "url" : "https://bitbucket.org/calderadev/cf-users"
    },
    {
      "type": "git",
      "url" : "https://bitbucket.org/calderadev/caldera-custom-login"
    }
  ],
  "require"     : {
    "johnpbloch/wordpress"                                      : "4.2.2",
    "pods-framework/pods"                                       : "2.x",
    "wpackagist-plugin/caldera-forms"                           : "*",
    "wpackagist-plugin/restricted-site-access"                  : "*",
    "wpackagist-plugin/log-viewer"                              : "*",
    "wpackagist-plugin/caldera-forms"                           : "*",
    "wpackagist-theme/twentyfifteen"                            : "*",
    "calderawp/caldera-custom-login"                            : "*",
    "calderawp/cf-users"                                        : "*",
    "calderawp/caldera-easy-pods"                               : "*"
  },
  "require-dev" : {
    "wpackagist-plugin/log-deprecated-notices"              : "*",
    "wpackagist-plugin/debug-bar"                           : "*",
    "wpackagist-plugin/debug-bar-console"                   : "*",
    "wpackagist-plugin/user-switching"                      : "*",
    "wpackagist-plugin/simply-show-ids"                     : "*"
  },
  "extra"       : {
    "wordpress-install-dir": "public_html/wp",
    "installer-paths": {
      "public_html/foundri-mvp-content/plugins/{$name}/" : ["type:wordpress-plugin"],
      "public_html/foundri-mvp-content/themes/{$name}/"  : ["type:wordpress-theme"]
    }
  }
}
