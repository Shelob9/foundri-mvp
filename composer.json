{
  "name"        : "shelob9/foundri-mvp",
  "description" : "foundri mvp.",
  "authors"     : [
    {
      "name"    : "Foundri",
      "email"   : "Josh@JoshPress.net",
      "homepage": "http://Foundri.co"
    }
  ],
  "type"        : "project",
  "minimum-stability": "dev",
  "repositories": [
    {
      "type": "composer",
      "url" : "http://wpackagist.org"
    },
    {
      "type": "git",
      "url": "https://github.com/pods-framework/pods"
    },
    {
      "type": "git",
      "url" : "https://bitbucket.org/calderadev/cf-users"
    },
    {
      "type" : "git",
      "url"  : "https://github.com/WP-API/WP-API"
    },
    {
      "type" : "git",
      "url" : "https://github.com/Desertsnowman/Caldera-Forms"
    }
  ],
  "require"     : {
    "johnpbloch/wordpress"                                      : "4.2.2",
    "pods-framework/pods"                                       : "2.x",
    "wpackagist-plugin/caldera-forms"                           : "*",
    "wpackagist-theme/twentyfifteen"                            : "*",
    "calderawp/cf-users"                                        : "*",
    "wp-api/wp-api"                                             : "*",
    "Desertsnowman/Caldera-Forms"                               : "1.2.x"
  },
  "extra"       : {
    "wordpress-install-dir": "wp",
    "installer-paths": {
      "foundri-mvp-content/plugins/{$name}/" : ["type:wordpress-plugin"],
      "foundri-mvp-content/themes/{$name}/"  : ["type:wordpress-theme"]
    }
  }
}
